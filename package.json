    "@types/webpack-env": "^1.16.2",
    "autoprefixer": "^10.3.0",
    "eslint": "^7.29.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "husky": "^6.0.0",
    "prettier": "^2.3.2",
    "sass": "^1.35.2",
    "sass-loader": "10",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0"
  }
