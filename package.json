    "moment": "^2.29.1",
    "nuxt": "^2.15.7",
    "nuxt-i18n": "^6.27.2",
    "qs": "^6.10.1",
    "slug": "^5.1.0",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/dotenv": "^1.4.1",
