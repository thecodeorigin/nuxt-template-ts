{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["ESNext", "ESNext.AsyncIterable", "DOM"],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@": ["./index.js", "./index.json", "./index.vue", "./index.scss", "./index.css"],
      "~/*": ["./*"],
      "~": ["./index.js", "./index.json", "./index.vue", "./index.scss", "./index.css"],
      "@@/*": ["./*"],
      "@@": ["./index.js", "./index.json", "./index.vue", "./index.scss", "./index.css"],
      "~~/*": ["./*"],
      "~~": ["./index.js", "./index.json", "./index.vue", "./index.scss", "./index.css"],
      "@core/*": ["./core/*"],
      "@core": [
        "./core/index.js",
        "./core/index.json",
        "./core/index.vue",
        "./core/index.scss",
        "./core/index.css"
      ],
      "@services/*": ["./services/*"],
      "@services": [
        "./services/index.js",
        "./services/index.json",
        "./services/index.vue",
        "./services/index.scss",
        "./services/index.css"
      ],
      "@components/*": ["./components/*"],
      "@components": [
        "./components/index.js",
        "./components/index.json",
        "./components/index.vue",
        "./components/index.scss",
        "./components/index.css"
      ],
      "@store/*": ["./store/*"],
      "@store": [
        "./store/index.js",
        "./store/index.json",
        "./store/index.vue",
        "./store/index.scss",
        "./store/index.css"
      ],
      "@apis/*": ["./core/apis/*"],
      "@apis": [
        "./core/apis/index.js",
        "./core/apis/index.json",
        "./core/apis/index.vue",
        "./core/apis/index.scss",
        "./core/apis/index.css"
      ],
      "@constants/*": ["./core/constants/*"],
      "@constants": [
        "./core/constants/index.js",
        "./core/constants/index.json",
        "./core/constants/index.vue",
        "./core/constants/index.scss",
        "./core/constants/index.css"
      ],
      "@mixins/*": ["./core/mixins/*"],
      "@mixins": [
        "./core/mixins/index.js",
        "./core/mixins/index.json",
        "./core/mixins/index.vue",
        "./core/mixins/index.scss",
        "./core/mixins/index.css"
      ],
      "@types/*": ["./core/types/*"],
      "@types": [
        "./core/types/index.js",
        "./core/types/index.json",
        "./core/types/index.vue",
        "./core/types/index.scss",
        "./core/types/index.css"
      ],
      "@utils/*": ["./core/utils/*"],
      "@utils": [
        "./core/utils/index.js",
        "./core/utils/index.json",
        "./core/utils/index.vue",
        "./core/utils/index.scss",
        "./core/utils/index.css"
      ]
    },
    "types": ["@nuxt/types", "@nuxtjs/axios", "@types/node", "@types/webpack-env"]
  },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
